<?xml version="1.0" encoding="UTF-8"?>
<NamedCredential xmlns="http://soap.sforce.com/2006/04/metadata">
    <allowMergeFieldsInBody>false</allowMergeFieldsInBody>
    <allowMergeFieldsInHeader>false</allowMergeFieldsInHeader>
    <awsAccessKey>{env.AWS_ACCESS_KEY}</awsAccessKey>
    <awsAccessSecret>{env.AWS_ACCESS_SECRET}</awsAccessSecret>
    <awsRegion>us-east-1</awsRegion>
    <awsService>s3</awsService>
    <calloutStatus>Enabled</calloutStatus>
    <endpoint>{env.AWS_LEGACY_URL}</endpoint>
    <generateAuthorizationHeader>true</generateAuthorizationHeader>
    <label>AWS</label>
    <principalType>NamedUser</principalType>
    <protocol>AwsSv4</protocol>
</NamedCredential>
